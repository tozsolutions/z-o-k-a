# Zoka Game - Environment Variables

# Application
VITE_APP_TITLE="Zoka - En İyi Çiftler Oyunu"
VITE_APP_DESCRIPTION="Partnerinizle veya arkadaşlarınızla unutulmaz anlar yaşayın."
VITE_APP_VERSION="1.0.0"

# Analytics (Optional - uncomment and configure as needed)
# VITE_GOOGLE_ANALYTICS_ID=
# VITE_GOOGLE_TAG_MANAGER_ID=

# Error Tracking (Optional - uncomment and configure as needed)
# VITE_SENTRY_DSN=

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_PWA=true

# API Configuration (if needed in future)
# VITE_API_BASE_URL=https://api.zoka.app
# VITE_API_VERSION=v1

# Social Media
VITE_SOCIAL_TWITTER="@zoka_game"
VITE_SOCIAL_INSTAGRAM="@zoka_game"

# Contact
VITE_CONTACT_EMAIL="support@zoka.app"
VITE_CONTACT_WEBSITE="https://zoka.app"

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_REDIRECT=true